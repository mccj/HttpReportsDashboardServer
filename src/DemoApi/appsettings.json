{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "HttpReports": {
    "Transport": {
      "CollectorAddress": "http://localhost:32354/",
      "DeferSecond": 10,
      "DeferThreshold": 100
    },
    "Server": "",
    "Service": "mccj",
    "Switch": true,
    "RequestFilter": [ "/health/*" ],
    "WithRequest": true,
    "WithResponse": true,
    "WithCookie": true,
    "WithHeader": true
  }
}
